f = input()
if f == ("треугольник"):
    a = float (input())
    b = float (input())
    c = float (input())
    p = (a + b + c) / 2
    s = (p * (p - a)*(p - b)*(p - c))**0.5
    print (s)
elif f == ("прямоугольник"):
        a = float (input())
        b = float (input())
        print (a*b)
elif f == ("круг"):
        a = float (input())
        print (3.14*(a**2))
# - бути уважним до Табів) 
